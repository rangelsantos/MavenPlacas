<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="247.0" prefWidth="505.0" styleClass="backgroundMain" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.PrimaryController">
    <children>
      <Button fx:id="btnSave" alignment="CENTER_RIGHT" layoutX="228.0" layoutY="173.0" onAction="#getSave" onKeyPressed="#keyListener" prefHeight="60.0" prefWidth="60.0" textAlignment="CENTER">
          <tooltip>
              <Tooltip text="Salvar" />
          </tooltip>
      </Button>
      <Button fx:id="btnPrint" alignment="CENTER_RIGHT" layoutX="324.0" layoutY="173.0" mnemonicParsing="false" onAction="#getPrint" onKeyPressed="#keyListener" prefHeight="60.0" prefWidth="60.0" textAlignment="CENTER"> 
          <tooltip>
              <Tooltip text="Imprimir" />
          </tooltip>
      </Button>
      <ComboBox fx:id="cbGroup" layoutX="116.0" layoutY="42.0" onAction="#cbGroupListener" prefHeight="25.0" prefWidth="100.0" promptText="Grupo">
          <tooltip>
              <Tooltip text="Grupo/Tipo" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbBrand" layoutX="291.0" layoutY="42.0" onAction="#cbBrandListener" prefHeight="25.0" prefWidth="100.0" promptText="Marca">
          <tooltip>
              <Tooltip text="Marca" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbSmallSize" layoutX="168.0" layoutY="134.0" onAction="#cbSmallSizeListener" prefWidth="100.0" promptText="Menor">
          <tooltip>
              <Tooltip text="Tamanho menor" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbLargeSize" layoutX="343.0" layoutY="134.0" onAction="#cbLargeSizeListener" prefWidth="100.0" promptText="Maior">
          <tooltip>
              <Tooltip text="Tamanho maior" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbStyle" layoutX="221.0" layoutY="42.0" prefHeight="25.0" prefWidth="64.0" promptText="-">
          <tooltip>
              <Tooltip text="Sinal separador" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbKidsSmallSize" layoutX="168.0" layoutY="88.0" onAction="#cbSmallSizeKidsListener" prefWidth="100.0" promptText="Menor">
          <tooltip>
              <Tooltip text="Tamanho menor da Puramania Mini Me" />
          </tooltip>
      </ComboBox>
      <ComboBox fx:id="cbKidsLargeSize" layoutX="343.0" layoutY="88.0" onAction="#cbLargeSizeKidsListener" prefWidth="100.0" promptText="Maior">
          <tooltip>
              <Tooltip text="Tamanho maior da Puramania Mini Me" />
          </tooltip>
      </ComboBox>
      <TextField fx:id="txtModel" layoutX="396.0" layoutY="42.0" prefHeight="26.0" prefWidth="100.0" promptText="Modelo">
          <tooltip>
              <Tooltip text="Modelo/Produto/Referência" />
          </tooltip>
      </TextField>
      <Button id="close" fx:id="closeWindow" layoutX="489.0" mnemonicParsing="false" onAction="#buttonClose" stylesheets="@style.css">
          <tooltip>
              <Tooltip text="Fechar" />
          </tooltip>
      </Button>
      <Button id="minimize" fx:id="minimizeWindow" layoutX="465.0" mnemonicParsing="false" onAction="#buttonMinimize">
          <tooltip>
              <Tooltip text="Minimizar" />
          </tooltip>
      </Button>
      <Button id="btnHelp" fx:id="btnHelp" layoutX="479.0" layoutY="209.0" mnemonicParsing="false" onAction="#aboutMensage">
          <tooltip>
              <Tooltip text="Sobre" />
          </tooltip>
      </Button>
      <CheckBox fx:id="useXG" layoutX="27.0" layoutY="113.0" mnemonicParsing="false" onAction="#setXG" text="XG">
          <tooltip>
              <Tooltip text="Usar XG ao invés de EG" />
          </tooltip>
      </CheckBox>
      <CheckBox fx:id="use34" layoutX="27.0" layoutY="136.0" mnemonicParsing="false" onAction="#set34" text="34-36">
