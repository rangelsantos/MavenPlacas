            <Tooltip text="Usar 34-36 ao invés de 36" />
         </tooltip>
      </CheckBox>
      <CheckBox fx:id="use50" layoutX="27.0" layoutY="159.0" mnemonicParsing="false" onAction="#set50" text="50-52">
         <tooltip>
            <Tooltip text="Usar 50-52 ao invés de 50" />
         </tooltip>
      </CheckBox>
      <CheckBox fx:id="usePPP" layoutX="27.0" layoutY="67.0" mnemonicParsing="false" onAction="#setPPP" text="PP-P">
          <tooltip>
            <Tooltip text="Usar PP-P ao invés de P" />
         </tooltip>
      </CheckBox>
      <CheckBox fx:id="useGGG" layoutX="27.0" layoutY="90.0" mnemonicParsing="false" onAction="#setGGG" text="G-GG">
          <tooltip>
            <Tooltip text="Usar G-GG ao invés de G" />
         </tooltip>
      </CheckBox>
      <Label layoutX="202.0" layoutY="7.0" text="Projeto Placas (1.0-rc.1)" textFill="WHITE" />
      <Label fx:id="iconImage" layoutX="4.0" layoutY="5.0" />
    </children>
</AnchorPane>
